# Uncomment the next line to define a global platform for your project
platform :osx, '10.13'

def pods
  use_frameworks!

  # Pods for HackQ
  pod 'Alamofire'
  pod 'AlamofireObjectMapper'
  pod 'SwiftyJSON'
  pod 'SwiftWebSocket'
  pod 'KeychainSwift'
  pod 'ObjectMapper'
  pod 'Starscream'
end

target 'HackQ' do
  pods
end

target 'HackQTests' do
  pods
end