# Uncomment the next line to define a global platform for your project
platform :osx, '10.13'

def pods
  use_frameworks!

  # Pods for Trivia Swift
  pod 'Alamofire'
  pod 'AlamofireObjectMapper'
  pod 'SwiftyJSON'
  pod 'SwiftWebSocket'
  pod 'KeychainSwift'
  pod 'ObjectMapper'
  pod 'Starscream'
  pod 'HotKey'
end

target 'Trivia Swift' do
  pods
end

target 'Trivia SwiftTests' do
  pods
end
